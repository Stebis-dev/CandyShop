<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="244.0" prefWidth="209.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kursinis.CandyShop.fxControllers.LoginController">
    <children>
        <TextField fx:id="loginField" layoutX="30.0" layoutY="38.0" prefHeight="26.0" prefWidth="150.0"
                   promptText="Enter login:" AnchorPane.bottomAnchor="180.0" AnchorPane.leftAnchor="30.0"
                   AnchorPane.rightAnchor="29.0" AnchorPane.topAnchor="38.0"/>
        <PasswordField fx:id="passwordField" layoutX="29.0" layoutY="79.0" promptText="Enter password:"/>
        <Button layoutX="54.0" layoutY="138.0" mnemonicParsing="false" onAction="#validateAndConnect" prefHeight="26.0"
                prefWidth="98.0" text="Login"/>
        <Button layoutX="54.0" layoutY="185.0" mnemonicParsing="false" onAction="#registerNewUser" prefHeight="26.0"
                prefWidth="98.0" text="Register"/>
    </children>
</AnchorPane>
